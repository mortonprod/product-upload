    "axios": "^0.16.2",
    "babel-jest": "^21.0.2",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "body-parser": "^1.18.2",
    "chai": "^4.1.2",
    "clean-webpack-plugin": "^0.1.16",
    "coveralls": "^2.13.1",
