{
  "name": "@mortonprod/product-upload",
  "version": "1.0.3",
  "description": "This is an npm package which acts as a node server to upload products to multiple servers.",
  "keywords": [],
  "author": "Alexander Morton <mortonprod@gmail.com>",
  "homepage": "https://github.com/mortonprod/product-upload#readme",
  "main": "./dist/index.min.js",
  "types": "./dist/index.d.ts",
  "module": "./dist/lib/index.js",
  "es2015": "./dist/lib-esm/index.js",
  "scripts": {
    "prepublish": "npm run build",
    "scrape": "node --max_old_space_size=2000000  scrapper.js",
    "docs": "typedoc --tsconfig ./tsconfig.json --out docs",
    "build:es5": "tsc --outDir ./dist/lib",
    "build:es6": "tsc -m es6 --outDir ./dist/lib-esm",
    "build:umd": "webpack --config ./webpack/webpack.prod.js --display-error-details",
    "build": "npm run build:umd && npm run build:es5 && npm run build:es6",
    "watch": "webpack-dev-server --open --config ./webpack/webpack.dev.js",
    "server:build": "NODE_ENV=production docker-compose build",
    "server": "NODE_ENV=development  docker-compose up",
    "server:restart": "NODE_ENV=development && docker stop $(docker ps -a -q) && docker-compose up",
    "server:watch": "NODE_ENV=development ./node_modules/nodemon/bin/nodemon.js ./server.js localhost 3000",
    "test": "npm run build && jest ./__tests__/*",
    "test:watch": "npm run build && jest --watch ./__tests__/*",
    "test:coverage": "npm run build && jest ./__tests__/ --coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"   
  },
  "jest": {
    "snapshotSerializers": [
      "<rootDir>/node_modules/enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "^.+\\.(css|scss)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "express": "^4.16.2",
    "mongodb": "^2.2.33",
    "multer": "^1.3.0",
    "react": "^15.3.2",
    "react-dom": "^15.3.2",
    "react-router-dom": "^4.2.2",
    "ts-node": "^3.3.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/body-parser": "^1.16.5",
    "@types/chai": "^4.0.4",
    "@types/enzyme": "^2.8.8",
    "@types/express": "^4.0.37",
    "@types/mocha": "^2.2.43",
    "@types/mongodb": "^2.2.13",
    "@types/multer": "^1.3.3",
    "@types/node": "^8.0.28",
    "@types/react": "^16.0.5",
    "@types/react-dom": "^16.0.1",
    "@types/react-router-dom": "^4.0.8",
    "@types/sinon": "^2.3.5",
    "@types/website-scraper": "^1.2.2",
    "autoprefixer": "^7.1.4",
